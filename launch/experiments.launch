<launch>
  <include file="$(find baxter_command_msg_conversion)/launch/msg_conversion.launch"/>
  <node pkg="coordination_experiments" type="coordination_controller" name="coordination_controller" output="screen">
    <remap from="joint_states" to="/robot/joint_states"/>
    <rosparam command="load" file="$(find coordination_experiments)/config/experiment.yaml"/>
  </node>
  <node pkg="object_server" type="object_server" name="object_server" output="screen"/>
</launch>
